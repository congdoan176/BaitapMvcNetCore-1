
@{
    ViewData["Title"] = "Create";
}

<h2>Add Student</h2>
<form method="post" action="Store" class="form-group">
    <div class="input-group" style="margin:20px">
        <span class="glyphicon glyphicon-user input-group-addon"></span>
        <input type="text" name="name" value="" class="form-control" />
    </div>
    <div class="input-group" style="margin:20px">
        <span class="glyphicon glyphicon-minus input-group-addon"></span>
        <input type="text" name="RollNumber" value="" class="form-control"/>
    </div>
    <input type="submit" name="submit" value="Submit" style="margin:20px" />
</form>

